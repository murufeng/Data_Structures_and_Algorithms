void VisitList(LinkList L)  
{  
    LinkList p;  
    p = L->next;  
    if (p==NULL)  
    {  
        cout<<"0"<<endl;  
    }  
    while (p)  
    {  
        cout<<p->factor<<"X^"<<p->indice;  
        p = p->next;  
        if(p != NULL)  
            cout<<"+";  
    }  
    cout<<endl;  
}  
